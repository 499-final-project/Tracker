{
  "rules": {
    "users": {
      ".indexOn": ["email"],
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "tasks": {
      ".indexOn": ["Taskname"],
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
        
      }
    }
  }
}